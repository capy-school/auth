{
  "name": "api",
  "type": "module",
  "scripts": {
    "dev": "vite | pino-pretty",
    "build": "vite build",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "queue:dev": "npx @upstash/qstash-cli dev"
  },
  "dependencies": {
    "@hono/zod-openapi": "^1.1.4",
    "@libsql/kysely-libsql": "^0.4.1",
    "@scalar/hono-api-reference": "^0.9.24",
    "better-auth": "^1.3.34",
    "hono": "^4.10.4",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.14.0",
    "vite": "^7.2.2",
    "vite-ssr-components": "^0.5.1",
    "wrangler": "^4.46.0"
  }
}